
import java.util.Scanner;


public class Main {

    
    
    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        
        System.out.print("Lütfen bir sayı giriniz: ");
        int seri = sc.nextInt();
        int a=0,b =1;
        
        for (int i = 1; i <= seri; i++) {
            
           System.out.print(a+" ");
           int toplam = a+b; 
           a=b;
           b=toplam;
           
            
             
        }
        
    }
}
