import java.util.Scanner;

public class Main {

 
    public static void main(String[] args) {
     
        Scanner sc = new Scanner(System.in);
        
        int a;
        
        System.out.print("Lütfen bir sayı giriniz: ");
        a = sc.nextInt();
        
        for (int i = 2; i <= a; i= i+2) {
            
            System.out.print(i +",");
            
            
        }
                
        
    }
    
}
